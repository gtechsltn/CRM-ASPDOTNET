<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="ADAccountSettings" type="System.Configuration.NameValueSectionHandler, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
  </configSections>

  <!-- GUIDE FOR USE <ADAccountSettings> -->

  <!--

  Before run WebApplication1.ConsoleApp.exe:

  <ADAccountSettings>
    <add key="ADDomainUserName" value="Administrator" />
    <add key="ADDomainPassword" value="Password@8" />
  </ADAccountSettings>

  After run WebApplication1.ConsoleApp.exe:

  <ADAccountSettings configProtectionProvider="DataProtectionConfigurationProvider">
    <EncryptedData>
      <CipherData>
        <CipherValue></CipherValue>
      </CipherData>
    </EncryptedData>
  </ADAccountSettings>

  -->

  <!-- PLEASE ENTER <ADDomainUserName>, <ADDomainPassword> HERE -->

  <!--<ADAccountSettings>
    <add key="ADDomainUserName" value="" />
    <add key="ADDomainPassword" value="" />
  </ADAccountSettings>-->

  <ADAccountSettings>
    <add key="ADDomainUserName" value="Administrator" />
    <add key="ADDomainPassword" value="Password@8" />
  </ADAccountSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>